<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">




	<bean id="tenis" class="org.angular.model.entities.Sport">
		<property name="description" value="tenis" />
	</bean>
	<bean id="futbol" class="org.angular.model.entities.Sport">
		<property name="description" value="futbol" />
	</bean>
	<bean id="baloncesto" class="org.angular.model.entities.Sport">
		<property name="description" value="baloncesto" />
	</bean>
	<bean id="atletismo" class="org.angular.model.entities.Sport">
		<property name="description" value="atletismo" />
	</bean>

	<bean id="sportList" class="java.util.ArrayList">
		<constructor-arg>
			<list>
				<ref bean="tenis" />
				<ref bean="futbol" />
				<ref bean="atletismo" />
				<ref bean="baloncesto" />
			</list>
		</constructor-arg>
	</bean>

	<bean id="person1" class="org.angular.model.entities.Person">
		<property name="name" value="Francisco" />
		<property name="surname" value="Abad" />
		<property name="login" value="fabad" />
		<property name="age" value="26" />
	</bean>
</beans>
